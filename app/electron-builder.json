{
  "appId": "schorty.schnecklauncher",
  "publish": [{
    "provider": "generic",
    "url": "https://schlenger.me/launcherfiles"
  }],
  "win": {
    "target": "nsis",
    "icon": "assets/app.ico",
    "verifyUpdateCodeSignature": false
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  },
  "files": [
    "!**/dev-app-update-sample.yml",
    "!**/dev-app-update.yml",
    "!**/schnecke/**/*",
    "!**/schnecke/*"
  ]
}